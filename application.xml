<?xml version="1.0" encoding="utf-8"?>
<project>
<!-- NMML reference: https://gist.github.com/1763850 -->
<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
<meta title="Main" package="lib.hxDaedalus.simpledrawing" version="1.0.0" company="JLM" />
<!-- output -->
<app main="Main" path="bin" file="Main" />
<!-- window properties -->
<window background="#FFFFFF" fps="60" />
<window width="600" height="400" unless="mobile" />
<window orientation="landscape" vsync="false" antialiasing="4" if="cpp" />
<set name="SWF_VERSION" value="11.2" />
<!-- classpath, haxe libs -->
<source path="src" />
<haxelib name="openfl" />
<haxelib name="hxDaedalus" />
<haxelib name="hxPixels" />
<!-- optimize output
<haxeflag name="-dce full" /> -->
</project>
